<section>
  <h1>Profile</h1>
  <div class="row">
    {{#each model as |profile|}}
        <div><img src="{{profile.picture}}" alt="Profile picture"></div>
    <div class="container">
        <span><strong>User id:</strong></span>
        <span>{{profile._id}}</span>
        <br>
        <span><strong>Nombre:</strong></span>
        <span>{{profile.name}}</span>
        <br>
        <span><strong>Email:</strong></span>
        <span>{{profile.email}}</span>
        <br>
        <span><strong>Business:</strong></span>
        <span>{{profile.business}}</span>
    </div>
  {{/each}}
  </div>
</section>